
// 7/15级加半次
IF
	Kit(Player%slot%,c4Q06Thief)
	LevelGT(Player%slot%,6)
	LevelLT(Player%slot%,15)
	CheckStatLT(Player%slot%,8,PROFICIENCYDAGGER)
THEN
	RESPONSE #100
		ReallyForceSpellRES("c4QCAPR1",Player%slot%)
		Continue()
END
	
IF
	Kit(Player%slot%,c4Q06Thief)
	LevelGT(Player%slot%,14)
	CheckStatLT(Player%slot%,16,PROFICIENCYDAGGER)
THEN
	RESPONSE #100
		ReallyForceSpellRES("c4QCAPR2",Player%slot%)
		Continue()
END
	